{
  "name": "@kwhitley/localstorify",
  "version": "0.1.5",
  "description": "localStorage that avoids storage limit failures",
  "keywords": [
    "localStorage",
    "garbage collection",
    "managed"
  ],
  "main": "index.js",
  "target": "esnext",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "babel src --ignore '**/*.spec.js' --out-dir build -d",
    "test": "jest --watch",
    "release:major": "yarn build && release --major --src=build --push --public",
    "release:minor": "yarn build && release --minor --src=build --push --public",
    "release:patch": "yarn build && release --patch --src=build --push --public"
  },
  "author": "Kevin R. Whitley <krwhitley@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwhitley/localstorify.git"
  },
  "bugs": {
    "url": "https://github.com/kwhitley/localstorify/issues",
    "email": "krwhitley@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@kwhitley/yarn-release": "^1.4.0",
    "jest": "^24.7.1"
  }
}
